@prefix as: <https://www.w3.org/ns/activitystreams#>.
@prefix ev: <https://www.eventnotifications.net/ns#>.
@prefix notify: <https://purl.org/coar/notify_vocabulary/>.
@prefix tree: <https://w3id.org/tree#>.
@prefix ldes: <https://w3id.org/ldes#>.
@prefix iana: <http://www.iana.org/assignments/relation/>.
@prefix dataid: <http://dataid.dbpedia.org/ns/core#>.
@prefix dc: <http://purl.org/dc/terms/>.
@prefix schema: <http://schema.org/>.
@prefix test: <https://example.org/ns#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Check a local event log for service nodes. Test if all events
# a service node provided for an artifact are available in the 
# local event log.
{
    ?Artifact 
        a ev:Artifact ;
        ev:eventlog ?DNEventLog .

    ?SNEvent
        a ev:ServiceNodeEvent ;
        dataid:checksum ?Checksum .

    ?SCOPE log:notIncludes {
        ?DNEvent 
            a ev:DataNodeEvent ;
            dataid:checksum ?Checksum .
     } .
}
=>
{
    ?DNEventLog test:missing ?SNEvent .
} .